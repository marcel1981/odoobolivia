<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>  	
  	<template id="assets_backend" name="custom bar assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <link rel="stylesheet" href="/backend_custom_bo/static/src/less/variables.less"/>
                <link rel="stylesheet" href="/backend_custom_bo/static/src/less/bootswatch.less"/>
                <link rel="stylesheet" href="/backend_custom_bo/static/src/less/style.less"/>
                <script type="text/javascript" src="/backend_custom_bo/static/src/js/toggle_leftbar.js"></script>
            </xpath>
    </template>
    <template id="menu_webclient_bootstrap_backend_custom_bo" inherit_id="web.webclient_bootstrap">
            <xpath expr="//div[@class='navbar-header']" position="inside">
                <ul class="ul_toggle_leftmenu">
                    <li class="btn-link toggle_leftmenu">
                        <span class="fa fa-bars fa-lg"></span>
                    </li>
                </ul>
            </xpath>
        </template>
  </data>
</openerp>